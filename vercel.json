{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/tts",
      "dest": "/api/tts.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}